<% if (flashMessages.success_msg && flashMessages.success_msg != "") { %> 
<div class="alert alert-success" role="alert"><%= flashMessages.success_msg %></div>
<% } else if (flashMessages.error_msg && flashMessages.error_msg != "") { %> 
<div class="alert alert-danger" role="alert"><%= flashMessages.error_msg %></div>
<% } %> 